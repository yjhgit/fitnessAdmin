t\t\t\tcallback(new Error(\"请输入整数\"));\r\n\t\t\t} else {\r\n\t\t\t\tcallback();\r\n\t\t\t}\r\n\t\t};\r\n\t\treturn {\r\n\t\t\tid: '',\r\n\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\tro:{\r\n\t\t\t\tqicaimingcheng : false,\r\n\t\t\t\tqicaileixing : false,\r\n\t\t\t\tzujieshuliang : false,\r\n\t\t\t\tzulinshijian : false,\r\n\t\t\t\tzulinshuoming : false,\r\n\t\t\t\tzujieren : false,\r\n\t\t\t\tshouji : false,\r\n\t\t\t\tzhuangtai : false,\r\n\t\t\t},\r\n\t\t\t\r\n\t\t\t\r\n\t\t\truleForm: {\r\n\t\t\t\tqicaimingcheng: '',\r\n\t\t\t\tqicaileixing: '',\r\n\t\t\t\tzujieshuliang: '',\r\n\t\t\t\tzulinshijian: '',\r\n\t\t\t\tzulinshuoming: '',\r\n\t\t\t\tzujieren: '',\r\n\t\t\t\tshouji: '',\r\n\t\t\t\tzhuangtai: '',\r\n\t\t\t},\r\n\t\t\r\n\t\t\tqicaimingchengOptions: [],\r\n\t\t\tzhuangtaiOptions: [],\r\n\t\t\t\r\n\t\t\trules: {\r\n\t\t\t\tqicaimingcheng: [\r\n\t\t\t\t\t{ required: true, message: '器材名称不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tqicaileixing: [\r\n\t\t\t\t],\r\n\t\t\t\tzujieshuliang: [\r\n\t\t\t\t\t{ required: true, message: '租借数量不能为空', trigger: 'blur' },\r\n\t\t\t\t\t{ validator: validateIntNumber, trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tzulinshijian: [\r\n\t\t\t\t],\r\n\t\t\t\tzulinshuoming: [\r\n\t\t\t\t\t{ required: true, message: '租赁说明不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tzujieren: [\r\n\t\t\t\t\t{ required: true, message: '租借人不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tshouji: [\r\n\t\t\t\t\t{ required: true, message: '手机不能为空', trigger: 'blur' },\r\n\t\t\t\t\t{ validator: validateMobile, trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tzhuangtai: [\r\n\t\t\t\t\t{ required: true, message: '状态不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t}\r\n\t\t};\r\n\t},\r\n\tprops: [\"parent\"],\r\n\tcomputed: {\r\n\r\n\r\n\r\n\t},\r\n\tcreated() {\r\n\t\tthis.ruleForm.zulinshijian = this.getCurDateTime()\r\n\t},\r\n\tmethods: {\r\n\t\t\r\n\t\t// 下载\r\n\t\tdownload(file){\r\n\t\t\twindow.open(`${file}`)\r\n\t\t},\r\n\t\t// 初始化\r\n\t\tinit