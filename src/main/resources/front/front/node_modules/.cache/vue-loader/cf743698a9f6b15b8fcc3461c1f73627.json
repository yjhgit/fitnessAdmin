\" class=\"el-icon-back\">返回</el-button>\r\n        <div class=\"section-title\" :style='{\"margin\":\"10px 0\",\"fontSize\":\"20px\",\"lineHeight\":\"54px\",\"color\":\"#333\",\"textAlign\":\"center\",\"background\":\"#f5f5f5\"}'>我的预约</div>\r\n        <el-table :data=\"tableData\" style=\"width: 100%\">\r\n            <el-table-column label=\"教练姓名\" align=\"center\" width=\"200px\">\r\n                <template slot-scope=\"scope\">\r\n                    <span v-if=\"scope.row.type == 2\">{{ scope.row.price }}积分</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"姓名\">\r\n                <template slot-scope=\"scope\">\r\n                    <span v-if=\"scope.row.type == 1 || scope.row.type == 3\"><span :style='{\"fontSize\":\"12px\"}'>￥</span>{{ scope.row.price }}</span>\r\n                    <span v-if=\"scope.row.type == 2\">{{ scope.row.price }}积分</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"手机号\">\r\n                <template slot-scope=\"scope\">\r\n                    <span v-if=\"scope.row.type == 1 || scope.row.type == 3\"><span :style='{\"fontSize\":\"12px\"}'>￥</span>{{ scope.row.total }}</span>\r\n                    <span v-if=\"scope.row.type == 2\">{{ scope.row.total }}积分</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"预约时间\" prop=\"addtime\"></el-table-column>\r\n            <el-table-column label=\"操作\" width=\"150\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button type=\"success\" :style='{\"margin\":\"2px auto\",}' size=\"mini\" @click=\"confirm(scope.row)\">取消预约</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <el-pagination\r\n                background\r\n                class=\"pagination\"\r\n                :pager-count=\"7\"\r\n                :page-size=\"pageSize\"\r\n                :page-sizes=\"pageSizes\"\r\n                prev-text=\"<\"\r\n                next-text=\">\"\r\n                :hide-on-single-page=\"true\"\r\n                :layout='[\"total\",\"prev\",\"pager\",\"next\"].join()'\r\n                :total=\"total\"\r\n                :style='{\"padding\":\"10px\",\"margin\":\"10px auto\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"textAlign\":\"center\",\"width\":\"1200px\",\"fontWeight\":\"500\"}'\r\n                @current-change=\"curChange\"\r\n                @prev-click=\"prevClick\"\r\n                @next-click=\"nextClick\"\r\n        ></el-pagination>\r\n\r\n        <el-dialog title=\"物流信息\" :visible.sync=\"logisticsVisible\" width=\"30%\">\r\n            <div v-html=\"logisticsText\"></div>\r\n        </el-dialog>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import config from '@/config/config'\r\n    export default {\r\n        data() {\r\n            return {\r\n                layouts: '',\r\n                baseUrl: config.baseUrl,\r\n                activeName: '未支付',\r\n                tableData: [],\r\n                total: 1,\r\n                pageSize: 10,pageSizes: [10,20,30,50],\r\n                totalPage: 1,\r\n                session: {},\r\n                userTableName: localStorage.getItem('UserTableName'),\r\n                logisticsVisible: false,\r\n                logisticsText: ''\r\n            }\r\n        },\r\n        created() {\r\n            this.getSession();\r\n            this.getMyOrderList(1);\r\n        },\r\n        methods: {\r\n            backClick() {\r\n                this.$router.push('/index/center')\r\n            },\r\n            getSession() {\r\n                this.$http.get(this.userTableName+'/session', {emulateJSON: true}).then(res => {\r\n                    if (res.data.code == 0) {\r\n                        this.session = res.data.data;\r\n                    }\r\n                });\r\n            },\r\n            getMyOrderList(page) {\r\n                this.$http.get('jiaolianyuyue/page', {params: {page, limit: this.pageSize, yonghuming: localStorage.getItem('userid')}}).then(res => {\r\n                    if (res.data.code == 0) {\r\n                        this.tableData = res.data.data.list;\r\n                        this.total = res.data.data.total;\r\n                        this.pageSize = res.data.data.pageSize;this.pageSizes = [this.pageSize, this.pageSize*2, this.pageSize*3, this.pageSize*5];\r\n                        this.totalPage = res.data.data.totalPage;\r\n                    }\r\n                });\r\n            },\r\n            curChange(page) {\r\n                this.getMyOrderList(page);\r\n            },\r\n            prevClick(page) {\r\n                this.getMyOrderList(page);\r\n            },\r\n            nextClick(page) {\r\n                this.getMyOrderList(page);\r\n            }\r\n        }\r\n    }\r\n</script>\r\n"]}]}